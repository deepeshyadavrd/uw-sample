   */
   #smile{
    position: relative;
    animation: mymove 3s;
    animation-iteration-count: infinite;
  } 

  #smile path:nth-child(1){
    stroke-dasharray: 606; /* space */
     stroke-dashoffset: 606; /* position */
     animation: line-animation 50s ease forwards;
    animation-delay: 0.6s * 3; 
  }
 
  @keyframes line-animation {
    to {
        stroke-dashoffset: 0;
        
    }
  } 
  @keyframes mymove {
    0%   {top: 0px;}
    25%  { top: 0px;}
    50%  {top: 5px;}
    75%  {top: 0px;}
    100% {top: 0px;}
  }

