.news-later-form {
  position: relative;
  margin-top: 50px;
}

.news-later-form input[type="email"] {
  padding: 10px;
  border: none;
  border-bottom: 2px solid #ccc;
  font-size: 16px;
}

.news-later-form label {
  position: absolute;
  left: 10px;
  top: 18px;
  font-size: 16px;
  color: #aaa;
  transition: all 0.2s ease-out;
  pointer-events: none;
}

/* .news-later-form input[type="email"]:focus + label, .news-later-form input[type="email"]:active + label  {
    top: -15px;
    color: #666;
} */

.news-later-form input[type="email"]:focus + label,
.news-later-form input[type="email"]:not(:placeholder-shown) + label {
     top: -15px;
  font-size: 12px;
  color: #666;
}
.news-later-form input[type="email"]::-webkit-input-placeholder {
  /* WebKit/Blink browsers */
  color: transparent;
}

.news-later-form input[type="email"]:-moz-placeholder {
  /* Mozilla Firefox 4 to 18 */
  color: transparent;
}

.news-later-form input[type="email"]::-moz-placeholder {
  /* Mozilla Firefox 19+ */
  color: transparent;
}

.news-later-form input[type="email"]:-ms-input-placeholder {
  /* Internet Explorer 10-11 */
  color: transparent;
}

.news-later-form input[type="email"]::placeholder {
  /* Internet Explorer 11+ */
  color: transparent;
}
.news-later-form .submit-btn-news {
    position: absolute;
    top: 0px;
    right: 0px;
    border: none;
    background: none;
    color: #ff8000;
}
