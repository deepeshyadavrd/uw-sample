.reply-2-reply {
    width: 96%;
    margin-left: 2em;
    position: relative;
}
.reply-2-reply .message-cmt-box {
    background: #fffdf5;
    border-color: #ff800036;
}
.reply-2-reply::before {
    content: '';
    position: absolute;
    top: -13px;
    width: 49px;
    background: #ff000000;
    height: 82px;
    border-radius: 1px 0px 0px 13px;
    left: -25px;
    border-top: 1px solid transparent;
    border-left: 1px solid #9b9a9a;
    border-bottom: 1px solid #9b9a9a;
    border-right: 0px;
}
