  <script>
     $(document).ready(function(){
            $(".btn-minus").on("click",function(){
                var now = $(".countItem input").val();
                if ($.isNumeric(now)){
                    if (parseInt(now) -1> 0)
                    { now--;}
                    $(".countItem input").val(now);
                }
            })            
            $(".btn-plus").on("click",function(){
                var now = $(".countItem input").val();
                if ($.isNumeric(now)){
                    $(".countItem input").val(parseInt(now)+1);
                }
            })                        
        })
  </script>
