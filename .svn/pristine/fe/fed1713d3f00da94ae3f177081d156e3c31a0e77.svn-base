        var url = '?route=product/review';
        localStorage.setItem(key, id);
        if (localStorage.getItem("category") === null) {
            alert('here');
        }else{
            var c_value = localStorage.getItem("category")
            url += '&category=' + c_value;
        }
        if (localStorage.getItem("city") === null) {
            alert('here');
        }else{
            var ci_value = localStorage.getItem("city")
            url += '&city=' + ci_value;
        }
        if (typeof localStorage.getItem("date") !== 'undefined' && localStorage.getItem("date") !== null) {
            
            var date_value = localStorage.getItem("date")
            url += '&date=' + date_value;
            
        }else{
            alert(url);
            url += '';
        }
        location = url;
    }

        if(localStorage.getItem("category") != null){
        var lastSelected = localStorage.getItem('category');
        
            $('#categoryselect').val(lastSelected)
                .find("option[value='" + lastSelected +"']").attr('selected', true);
            
        }
        if(localStorage.getItem("city") != null){
        var lastCity = localStorage.getItem('city');
        
            $('#cityselect').val(lastCity)
                .find("option[value='" + lastCity +"']").attr('selected', true);
        }
        if(localStorage.getItem("date") != null){
        var lastDate = localStorage.getItem('date');
        
            $('#dateselect').val(lastDate)
                .find("option[value='" + lastDate +"']").attr('selected', true);
        }
</script>

