{% for order in orders %}

  <div class="col-md-12 col-sm-12">
<div class="panel-order-list">
{% for order_product in order.order_products %}
  <div class="row">
    <div class="col-lg-3 col-md-3 col-sm-12">
      <div class="pr-img-myorder">
        <img src="{{ order_product.image }}" class="">
        {# <img src="catalog/view/javascript/assets/image/order-list-img.jpg" class=""> #}
      </div>
    </div>

    <div class="col-lg-6 col-md-6 col-sm-12">
      <div class="my-panel-order-detail">
        <div class="pp-title-panel">{{ order_product.name }}</div>
        <div class="price--order">
          <div class="real-amm">₹24,332</div>
          <div class="dis-amm">₹35,035</div>
          <div class="off-amm">31% OFF</div>
        </div>
        <div class="pp-deatil-para">{{ order_product.description }}</div>
        {% if loop.last %}
        <ul class="delivery-add-detil">
          <li> <i class="fas fa-angle-right"></i> &nbsp; <span style="font-weight: 600;">Delivery Address : </span>{{ payment_address }}</li>
          <li> <i class="fas fa-angle-right"></i> &nbsp; <span  style="font-weight: 600;"> Store  Address : </span>{{ order.store_name }}</li>
        </ul>
        {% endif %}
      </div>
    </div>

    {% if loop.first %}
    <div class="col-lg-3 col-md-3 col-sm-12">
      <div class="info-order">
        <div class="panel-order-id"> Order ID :  <span>{{ order.order_id }} </span></div>
        <ul class="delivery-add-detil">
          <li> <i class="fas fa-angle-right"></i> &nbsp; <span> <b>Order Date : </b> </span> {{ order.date_added }}</li>
          <li> <i class="fas fa-angle-right"></i> &nbsp; <span class=""> <b> Deliver Date :</b> </span> 20, Jan, 2023</li>
        </ul>
      </div>
      </div>
    {% endif %}
    {% if loop.last %}
      <a class="btn btn-check-bill" data-bs-toggle="collapse" href="#collapseExample{{ order.order_id}}" role="button" aria-expanded="false" aria-controls="collapseExample{{ order.order_id}}">
        <i class="fas fa-angle-down"></i>
      </a>
    {% endif %}

  </div>
  {% endfor %}
  <div class="collapse bill-detail-down" id="collapseExample{{ order.order_id}}">
    <div class="card card-body ">
      <hr class="border-devider" style="border-bottom: #434342 1px dashed; background: none; border-top:none">
      <div class="bill-head">Bill Details</div>
    <div class="row">
<div class="col-lg-6 col-md-6 col-sm-12">
  <table class="table table-borderless">
    <tbody>
      <tr>
        <td class="bill-sec">Item No</td>
        <td>{{ order_product.quantity }}</td>

      </tr>
      <tr>
        <td class="bill-sec">Item  Total</td>
        <td>{{ order.total }}</td>
     
      </tr>
      <tr>
        <td class="bill-sec">Order Packing Charges <span><small>( Per Item )</small></span></td>
        <td>₹332</td>
        
  
      </tr>
      <tr>
        <td class="bill-sec">Order Packing Charges <span><small>( Per Item )</small></span></td>
        <td><span class="free-tag">FREE	</span></td>
        
  
      </tr>
      
      <tr>
        <td class="bill-sec">Taxes <span><small>( Per Item )</small></span></td>
        <td><span class="free-tag">FREE	</span></td>
  
      </tr>
      <tr class="bill-total-sub">
        <td>Bill Total</td>
        <td>{{ order.total }}</td>
      </tr>
      
    </tbody>
  </table>
</div>

  <div class="col-lg-6 col-md-6 col-sm-12 text-center pt-5 mt-5">
    <a href="trackorder.html"  target="_blank"  type="button" class="edit-btn">
      TRACK YOUR ORDER
    </a>
  </div>
    </div>
    </div>

  </div>
  
</div>

</div>

