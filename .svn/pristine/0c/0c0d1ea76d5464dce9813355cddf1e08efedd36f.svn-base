<!---modal for edit profile-->
<!-- Scrollable modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-for-profile-edit">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title manage-account" id="exampleModalLabel">Manage <span style="color:#ff8000; font-weight: 700;"> Account</span></h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
            <form class="form form-for-profile needs-validation" novalidate id="image-upload" action="?route=account/edit" method="post" enctype="multipart/form-data">
              <input type="hidden" id="max_file_size" name="max_file_size"  value="300000">
            
              <div class="form--field image-upload--headshot-image">
                {% if profile_img %}
                  <img src="{{ profile_img }}" class="upload" id='profileimg'>
                {% else %}
                  <img src="catalog/view/javascript/assets/image/profile-smap-img.png" class="upload" id='profileimg'>
                {% endif %}
                
                <input type="file" name="headshot" id="headshot" class="hidden-input-dp" required>
                <span class="profile-tg">Drag & Drop image,
                  or <strong>Browse </strong></span>
              </div>

            <div class="row">
            <div class="col-md-6 mb-3">
             
              <input type="text" class="form-control" id="firstnameprfile" aria-describedby="firstname" name="firstname" value="{{ firstname }}" placeholder="First Name" required>
              <div class="valid-feedback">
                Looks good!
              </div>
            </div>
            <div class="col-md-6 mb-3">
                <input type="text" class="form-control" id="firstlastnameprfile" aria-describedby="emailHelp" name="lastname" value="{{ lastname }}" placeholder="Last Name" required>
              </div>
              <div class="col-md-6 mb-3">
             
                <input type="email" class="form-control" id="emailprfile" aria-describedby="emailHelp" name="email" value="{{ email }}" placeholder="Email Address" required>
              </div>
              <div class="col-md-6 mb-3">
                  <input type="text" class="form-control " id="numberprfile" aria-describedby="emailHelp" name="telephone" value="{{ telephone }}" placeholder="Contact Number" required>
                </div>
            <div class="col-md-6 mb-3 mb-3">
              <input type="password" class="form-control" id="oldpassprofile" placeholder="Old Password" name="oldpass" required>
            </div>
            <div class="col-md-6 mb-3 mb-3">
              <input type="password" class="form-control" id="changepassproflie" name="newpass" placeholder="Change Password" required>
            </div>
           
            <div class="col-md-6 mb-3 mb-3">
              <input type="date" class="form-control" id="dob-user" name="dateofbirth" placeholder="date of birth" value="{{ dobforinput }}" required>
            </div>
            <div class="col-md-6 mb-3 mb-3">
              <select class="form-select form-select-lg mb-3" name="gender" aria-label="form-select-lg example" required>
                <option >Gender</option>
                <option value="Male" {% if gender == "Male" %} selected {% endif %}>Male</option>
                <option value="Female" {% if gender == "Female" %} selected {% endif %}>Female</option>
              </select>
            </div>

        </div>
        <div class="modal-footer">
          <button type="button" class="cancel-btn-profle" data-bs-dismiss="modal">Cancel</button>
          <button type="submit" class="update-btn-profile">Update</button>
        </div>
          </form>
        </div>
        
      </div>
    </div>
  </div>
</section> 
<script>
headshot.onchange = evt => {
  const [file] = headshot.files
  if (file) {
    profileimg.src = URL.createObjectURL(file)
  }
}
</script>
