{# {{ header }}
{{ menu }}
<div id="error-not-found" class="container">
    <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
            <li>
                <a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
            </li>
        {% endfor %}
    </ul>
    <div class="row">{{ column_left }}
        {% if column_left and column_right %}
            {% set class = 'col-sm-6' %}
        {% elseif column_left or column_right %}
            {% set class = 'col-sm-9' %}
        {% else %}
            {% set class = 'col-sm-12' %}
        {% endif %}
        <div id="content" class="{{ class }}">{{ content_top }}
            <h1>{{ heading_title }}</h1>
            <p>{{ text_error }}</p>
            <div class="buttons clearfix">
                <div class="pull-right">
                    <a href="{{ continue }}" class="btn btn-primary">{{ button_continue }}</a>
                </div>
            </div>
            {{ content_bottom }}</div>
        {{ column_right }}</div>
</div>
{{ footer }} #}
{{ header }}
{{ menu }}
<div class="wrapper">
<div class="qu-sec py-2 ">
  <div class="container container-custom">
    <div class="row">
<div class="col-md-12 col-sm-12">
  <nav class="breadcumb-navigation">
    <ol class="breadcrumb m-0 p-2">
      {# <li class="breadcrumb-item"><a href="#"><i class="fas fa-home"></i></a></li> #}
      {% for breadcrumb in breadcrumbs %}
      <li class="breadcrumb-item active" aria-current="page">{{ breadcrumb.text }}</li>
      {% endfor %}
    </ol>
  </nav>   
</div>
</div>
    </div>
</div>

<section class="text-center not-found-bg">
<div class="container-custom container">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="not-found-page">
                <div class="not-found-content">
                    {% if text_error1 %}
                        <h2>{{ text_error1 }}</h2>
                    {% else %}
                        <h2>OOPS!</h2>
                    {% endif %}
                    <h3 class="mb-3">{{ text_error }}</h3>
                     </div>
                <div class="not-found-img">
                    {% if page_text %}
                        <img src="catalog/view/javascript/assets/image/icon/empthy-cart-desktop.png" alt="" class="img-fluid">
                    {% else %}
                        <img src="catalog/view/javascript/assets/image/not-found.png" alt="" class="img-fluid">
                    {% endif %}
                </div>
                {% if page_text %}
                    <a href="{{ continue }}" class="goback-btn">Continue Shopping</a>
                {% else %}
                    <a href="{{ continue }}" class="goback-btn">Go Back!</a>
                {% endif %}
                
            </div>
        </div>
    </div>
</div>
</section>
</div>
{{ footer }}