// Helper function to validate name (no numbers allowed)
function validateName(name) {
    const re = /^[A-Za-z\s]+$/; // Allows only letters and spaces
    return re.test(name);
}
// Helper function to validate email format
function validateEmail(email) {
    const re = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
    return re.test(email);
}

// Helper function to validate 10-digit phone number format
function validatePhone(phone) {
    const re = /^(\+91[\-\s]?)?[0]?(91)?[789]\d{9}$/; // Matches exactly 10 digits
    return re.test(phone);
}

