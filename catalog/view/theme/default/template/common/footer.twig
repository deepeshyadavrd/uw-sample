<script>
   window.onscroll = function () { applySticky(); };

var desktopHeader = document.getElementById("myHeader");
var mobileHeader = document.getElementById("mobHeader");

var desktopSticky = desktopHeader.offsetTop;
var mobileSticky = mobileHeader.offsetTop;

function applySticky() {
  var width = window.innerWidth;

  // Sticky behavior for desktop (width > 992px)
  if (width > 992) {
    if (window.pageYOffset > desktopSticky) {
      desktopHeader.classList.add("sticky");
    } else {
      desktopHeader.classList.remove("sticky");
    }
    mobileHeader.classList.remove("sticky"); // Ensure mobile header doesn't stick
  }

  // Sticky behavior for mobile (width <= 992px)
  if (width <= 992) {
    if (window.pageYOffset > mobileSticky) {
      mobileHeader.classList.add("sticky");
    } else {
      mobileHeader.classList.remove("sticky");
    }
    desktopHeader.classList.remove("sticky"); // Ensure desktop header doesn't stick
  }
}
  </script>
  <script>
  document.addEventListener('DOMContentLoaded', function() {
    // Create a new <link> element
    const link = document.createElement('link');
    link.rel = 'stylesheet';
    link.href = 'https://cdnjs.cloudflare.com/ajax/libs/boxicons/2.1.4/css/boxicons.min.css'; // Replace with the actual path to your CSS file
    link.type = 'text/css';
    link.onload = () => console.log('Boxicon CSS loaded successfully.');

    // Append the <link> element to the <head>
    document.head.appendChild(link);
  });

  document.addEventListener('DOMContentLoaded', function () {
    const observer = new IntersectionObserver((entries, observer) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          // Dynamically show the modal when needed
          $('#signuppopup').modal('show');
          observer.unobserve(entry.target); // Stop observing
        }
      });
    });

    const modalTrigger = document.getElementById('signuppopup');
    observer.observe(modalTrigger);
  });
</script>
